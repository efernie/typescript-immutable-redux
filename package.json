    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.0",
    "redux-test-utils": "^0.1.2",
    "redux-thunk": "^2.2.0",
    "serve": "^5.1.1",
    "sinon": "^2.1.0",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
