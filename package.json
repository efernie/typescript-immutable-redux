{
  "name": "typescript-immutable-redux",
  "version": "1.0.0",
  "description": "example using typescript immutablejs and redux",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha src/**/*.test.{ts,tsx}",
    "test:coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "watch": "webpack --watch",
    "start": "serve",
    "devsrvr": "webpack-dev-server",
    "lint": "tslint --project tsconfig.json"
  },
  "nyc": {
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "exclude": [
      "test/*.js",
      "src/**/*.test.ts",
      "src/**/*.test.tsx"
    ]
  },
  "author": "Fernie <ericf2023@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^3.4.35",
    "@types/enzyme": "^2.7.6",
    "@types/isomorphic-fetch": "^0.0.33",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.11",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/react-redux": "^4.4.38",
    "@types/redux-immutable": "^3.0.33",
    "@types/redux-logger": "^2.10.0",
    "@types/sinon": "^1.16.36",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "enzyme": "^2.8.0",
    "enzyme-redux": "^0.1.6",
    "es6-promise": "^4.1.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.12.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.1.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
